  route-target export {{ h_bgp_community_id }}:167
  route-target import {{ h_bgp_community_id }}:167
 exit-address-family
 !
 address-family ipv6
  route-target export {{ h_bgp_community_id }}:167
  route-target import {{ h_bgp_community_id }}:167
